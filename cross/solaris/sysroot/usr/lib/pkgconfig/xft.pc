prefix=/usr/openwin
exec_prefix=/usr/openwin/bin
libdir=/usr/openwin/lib
includedir=/usr/openwin/include
freetypelibs=-L/usr/sfw/lib -R/usr/sfw/lib -lfreetype
freetypecflags=-I/usr/sfw/include -I/usr/sfw/include/freetype2
xrenderlibs=-L/usr/openwin/sfw/lib -R/usr/openwin/lib:/usr/openwin/sfw/lib -lXrender
xrendercflags=

Name: Xft
Description: X FreeType library
Version: 2.0.0
Requires: fontconfig
Libs: -L${libdir} -R${libdir} -lXft ${freetypelibs} ${xrenderlibs}
Cflags: -I${includedir} ${freetypecflags} ${xrendercflags}

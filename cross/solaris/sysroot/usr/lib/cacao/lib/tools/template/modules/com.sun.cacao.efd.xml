<module name="com.sun.cacao.efd" 
        initial-administrative-state="UNLOCKED"
        ignored-at-startup="No"
        enable-on-demand="Yes"
        version="2.0">

     <description>
        Event forwarder logic, does not register mbeans but
        provides classes for other modules to create and
        register EFD and FullLog MBeans
     </description>

     <public-path>
         <path-element>
           file:../../../../lib/cacao_efd.jar
         </path-element>
     </public-path>

     <cacao-version-supported>
           2.0
     </cacao-version-supported>
     <heap-requirements-kilobytes>
           100
     </heap-requirements-kilobytes>

</module>

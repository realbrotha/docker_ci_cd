    "prepare-uninstall")
	cacao_print_message "${CACAO_HLP_PREPUNIN_HEAD1}"
	cacao_print_message "${CACAO_HLP_PREPUNIN_HEAD2}"
	cacao_print_message "${CACAO_HLP_PREPUNIN_HEAD3}"
	cacao_print_message "${CACAO_HLP_HELP_________1}"
	;;
    *)
	cacao_command_usage
        ;;
    esac
    cacao_print_message "${CACAO_HLP_MAN__________1}"
}

#------------------------------------------------------------------------------
# cacao_command_usage
#
# DESCRIPTION:
# Display the cacaoadm command usage.
#
# PARAMETERS:
# None.
# 
# RETURN CODE:
# None.
# 
# OUTPUT:
# See description.
#------------------------------------------------------------------------------
cacao_command_usage() {

    if [ -z "${CACAO_HLP_CATALOG_ALREADY_SOURCED}" ] 
    then
	. ${cacao_tools_dir}/help_messages.cfg
    fi


    cacao_print_message "${CACAO_HLP_COMMAND_HEAD_1}"
    cacao_print_message "${CACAO_HLP_COMMAND_HEAD_2}"
    cacao_print_message "${CACAO_HLP_COMMAND_HEAD_3}"
    cacao_print_message "${CACAO_HLP_HELP_________1}"
    cacao_print_message "${CACAO_HLP_VERSION______1}"
    cacao_print_message "${CACAO_HLP_VERSION______2}"
    cacao_print_message "${CACAO_HLP_COMMAND_HEAD_4}"
    cacao_print_message "${CACAO_HLP_CMD__DISABLE_1}"
    cacao_print_message "${CACAO_HLP_CMD__DISABLE_2}"
    cacao_print_message "${CACAO_HLP_CMD___ENABLE_1}"
    cacao_print_message "${CACAO_HLP_CMD___ENABLE_2}"
    cacao_print_message "${CACAO_HLP_CMD___ENABLE_3}"
    cacao_print_message "${CACAO_HLP_CMD__RESTART_1}"
    cacao_print_message "${CACAO_HLP_CMD____START_1}"
    cacao_print_message "${CACAO_HLP_CMD_____STOP_1}"
    cacao_print_message "${CACAO_HLP_CMD___STATUS_1}"
    cacao_print_message "${CACAO_HLP_CMD_GETPARAM_1}"
    cacao_print_message "${CACAO_HLP_CMD_SETPARAM_1}"
    cacao_print_message "${CACAO_HLP_CMD_LSTPARAM_1}"
    cacao_print_message "${CACAO_HLP_CMD_LSTMODUL_1}"
    cacao_print_message "${CACAO_HLP_CMD_UNDEPLOY_1}"
    cacao_print_message "${CACAO_HLP_CMD_____LOCK_1}"
    cacao_print_message "${CACAO_HLP_CMD___UNLOCK_1}"
    cacao_print_message "${CACAO_HLP_CMD___DEPLOY_1}"
    cacao_print_message "${CACAO_HLP_CMD_GETFILTR_1}"
    cacao_print_message "${CACAO_HLP_CMD_SETFILTR_1}"
    cacao_print_message "${CACAO_HLP_CMD_LSTFILTR_1}"
    cacao_print_message "${CACAO_HLP_CMD_LSTFILTR_2}"
    cacao_print_message "${CACAO_HLP_CMD_CREATKEY_1}"
    cacao_print_message "${CACAO_HLP_CMD_LSTINSTA_1}"
    cacao_print_message "${CACAO_HLP_CMD_CR_INSTA_1}"

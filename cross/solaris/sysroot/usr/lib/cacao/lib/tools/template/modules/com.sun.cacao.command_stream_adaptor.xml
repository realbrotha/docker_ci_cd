<module name="com.sun.cacao.command_stream_adaptor" 
        version="1.0"
        initial-administrative-state="UNLOCKED"
        ignored-at-startup="No"
        enable-on-demand="No">

  <description>
    Adaptor providing TLS SASL/RBAC authenticated telnet-like interface.
  </description>

  <module-class>
    com.sun.cacao.commandstream.CommandStreamAdaptorModule
  </module-class>

  <public-path>
    <path-element>
      file:../../../../lib/cacao_commandstream.jar
    </path-element>
  </public-path>

  <cacao-version-supported>
    2.0
  </cacao-version-supported>

</module>

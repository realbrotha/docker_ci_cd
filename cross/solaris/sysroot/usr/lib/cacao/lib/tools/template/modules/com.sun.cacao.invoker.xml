<module name="com.sun.cacao.invoker" 
        initial-administrative-state="UNLOCKED"
        ignored-at-startup="No"
        enable-on-demand="No">
 <description>
    Remote command invocation interface, much like a non-interactive rsh
    but over a secure JMX connection.
 </description>

 <module-class>
      com.sun.cacao.invoker.impl.InvokerModule
 </module-class>

  <public-path>
      <path-element>
        file:../../../../lib/cacao_invoker.jar
      </path-element>
  </public-path>

  <cacao-version-supported>
       2.0
  </cacao-version-supported>
  <heap-requirements-kilobytes>
       100
  </heap-requirements-kilobytes>
</module>

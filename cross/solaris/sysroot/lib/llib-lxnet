/* LINTLIBRARY */
/* PROTOLIB1 */

/*
 * Set up the UNIX98 compilation environment.
 */
#define	_XOPEN_SOURCE	500
#include <sys/feature_tests.h>

/*
 * Slurp the interfaces out of the official UNIX98 networking headers.
 */
#include <sys/socket.h>
#include <arpa/inet.h>
#include <netinet/in.h>
#include <netdb.h>

/* Script for ld -r: link without relocation */
OUTPUT_ARCH(powerpc)
SECTIONS
{
  .pad 0 : { *(.pad) }
  . = ALIGN (0x10000000 + SIZEOF_HEADERS, 32);
  .text 0 : {
    *(.text)
    *(.pr)
    *(.ro)
    *(.db)
    *(.gl)
    *(.xo)
    *(.ti)
    *(.tb)
  }
  . = ALIGN (ALIGN (0x10000000) + (. & 0xfff), 32);
  .data . : {
    *(.data)
    *(.rw)
    *(.sv)
    *(.sv64)
    *(.sv3264)
    *(.ua)
    . = ALIGN(4);
    *(.ds)
    *(.tc0)
    *(.tc)
    *(.td)
  }
  .bss : {
    *(.tocbss)
    *(.bss)
    *(.bs)
    *(.uc)
    *(COMMON)
  }
  .loader : {
    *(.loader)
  }
  .debug : {
    *(.debug)
  }
}
